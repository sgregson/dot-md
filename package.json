{
  "name": "dotfiles-md",
  "type": "module",
  "version": "0.4.1",
  "license": "GPL-3.0-or-later",
  "author": "Spencer Gregson",
  "description": "Manage your dotfiles with markdown",
  "bin": "./cli.mjs",
  "main": "src/vorpal.mjs",
  "homepage": "https://github.com/sgregson/dotfiles-md#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/sgregson/dotfiles-md"
  },
  "keywords": [
    "dotfiles",
    "markdown",
    "cli"
  ],
  "engines": {
    "node": ">18"
  },
  "os": [
    "darwin"
  ],
  "scripts": {
    "release": "np",
    "prejfdi": "yarn",
    "jfdi": "cd dotfiles; node ../cli.mjs run -- --pattern \"**/*.md\" --actions symlink,build; cd -",
    "app": "node ./src/app.js",
    "doit": "cd dotfiles; node ../cli.mjs run -- --pattern \"**/*.md\" --actions symlink,build; cd -",
    "start": "node ./cli.mjs",
    "test": "echo \"STATUS: no test specified\"",
    "tsc": "tsc"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "np": {
    "branch": "main",
    "releaseDraft": true,
    "message": "Release %s"
  },
  "dependencies": {
    "@inquirer/prompts": "^5.0.7",
    "@moleculer/vorpal": "^1.11.5",
    "colors": "^1.4.0",
    "dotenv": "^16.0.3",
    "escape-string-regexp": "^5.0.0",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "inquirer": "^9.1.4",
    "inquirer-select-pro": "^1.0.0-alpha.6",
    "minimist-string": "^1.0.2",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "unified": "^10.1.2",
    "unist-util-visit": "^4.1.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "np": "^9.2.0",
    "typescript": "^5.5.2"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
