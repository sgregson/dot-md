{
  "name": "dotfiles-md",
  "version": "0.3.0",
  "license": "GPL-3.0-or-later",
  "author": "Spencer Gregson",
  "description": "Manage your dotfiles with markdown",
  "bin": "./cli.mjs",
  "main": "src/vorpal.mjs",
  "homepage": "https://github.com/sgregson/dotfiles-md#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/sgregson/dotfiles-md"
  },
  "keywords": [
    "dotfiles",
    "markdown",
    "cli"
  ],
  "engines": {
    "node": ">15"
  },
  "os": [
    "darwin"
  ],
  "scripts": {
    "release": "np",
    "prejfdi": "yarn",
    "jfdi": "cd dotfiles; node ../cli.mjs run -- --pattern \"**/*.md\" --actions symlink,build; cd -",
    "doit": "cd dotfiles; node ../cli.mjs run -- --pattern \"**/*.md\" --actions symlink,build; cd -",
    "start": "node ./cli.mjs",
    "test": "echo \"STATUS: no test specified\""
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "np": {
    "branch": "main",
    "releaseDraft": true,
    "message": "Release %s"
  },
  "dependencies": {
    "@moleculer/vorpal": "^1.11.5",
    "colors": "^1.4.0",
    "dotenv": "^16.0.3",
    "escape-string-regexp": "^5.0.0",
    "fs-extra": "^10.1.0",
    "glob": "^9.3.0",
    "inquirer": "^9.1.4",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "unified": "^10.1.2",
    "unist-util-visit": "^4.1.1"
  },
  "devDependencies": {
    "np": "^7.6.2"
  }
}
